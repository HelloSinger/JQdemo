<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <item app:tsquare_state_today="true" android:color="#ff0000"/>
  <item android:state_selected="true" app:tsquare_state_selectable="true" android:color="@color/calendar_text_selected"/>
  <item android:state_pressed="true" app:tsquare_state_selectable="true" android:color="@color/calendar_text_selected"/>
  <item app:tsquare_state_current_month="false" android:color="@color/white"/>
  <item app:tsquare_state_highlighted="true" android:color="@color/calendar_text_highlighted" />
  <item app:tsquare_state_selectable="false" android:color="@color/calendar_text_unselectable" />
  <item android:color="@color/calendar_text_active"/>
</selector>
